mechanisms = plain login
socket listen {
        client {
                path = /var/spool/postfix/private/dovecot-auth
                mode = 0660
                user = postfix
                group = postfix
        }
        master {
			          path = /var/run/dovecot/auth-master
			          mode = 0660
			          user = vmail
			          group = vmail
        }
}
